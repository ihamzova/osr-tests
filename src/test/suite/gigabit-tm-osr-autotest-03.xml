        </classes>
    </test>
    <test name="DPU-Commissioning cases (for SDX2221-16-TP device)" thread-count="1">
        <classes>
            <class name="com.tsystems.tm.acc.ta.domain.commissioning.DpuCommissioningSDX2221"/>
        </classes>
    </test>
    <test thread-count="1" name="A10nsp test cases">
        <classes>
            <class name="com.tsystems.tm.acc.ta.domain.a10nsp.CheckLineIdTest"/>
        </classes>
    </test>
</suite>
